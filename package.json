{
  "name": "d4l-wishlist",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "NEXT_WEBPACK_USEPOLLING=1 next dev",
    "dev:turbo": "next dev --turbo",
    "dev:https": "node scripts/start-https.js",
    "dev:https:simple": "node scripts/start-https-simple.js",
    "dev:https:direct": "HTTPS=true SSL_CRT_FILE=./certificates/localhost.pem SSL_KEY_FILE=./certificates/localhost-key.pem next dev -H main.d4l.ai -p 3000",
    "dev:debug": "node scripts/dev-debug.js",
    "dev:clean": "node scripts/start-clean.js",
    "dev:nginx": "node scripts/start-with-nginx.js",
    "generate-certs": "node scripts/generate-certificates.js",
    "update-hosts": "node scripts/update-hosts.js",
    "check-hosts": "node scripts/check-hosts.js",
    "kill-ports": "node scripts/kill-ports.js",
    "setup-https": "npm run generate-certs && npm run update-hosts",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "hardhat test",
    "typecheck": "tsc --noEmit",
    "clear-cache": "node scripts/clear-cache.js",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:full": "rm -rf .next node_modules && npm install",
    "optimize:npm": "node scripts/optimize-npm.js",
    "clean:deps": "rm -rf node_modules/.cache && npm cache clean --force",
    "install:fast": "npm install --prefer-offline --no-audit --no-fund",
    "ci:fast": "npm ci --prefer-offline --no-audit --no-fund --no-optional",
    "update:deps": "npm update --no-audit --no-fund",
    "analyze:deps": "node scripts/analyze-deps.js",
    "dedupe": "npm dedupe",
    "prune": "npm prune",
    "npm:speedup": "npm run optimize:npm && npm run clean:deps && npm run dedupe && npm run prune",
    "npm:fix": "node scripts/fix-npm-issues.js",
    "npm:reset": "rm -rf node_modules package-lock.json && npm cache clean --force && npm run npm:fix",
    "npm:registry": "node scripts/use-fast-registry.js",
    "npm:cache": "node scripts/setup-npm-cache.js",
    "npm:supercharge": "npm run npm:registry && npm run npm:cache && npm run npm:speedup && npm run npm:fix",
    "validate:next": "node scripts/validate-nextjs-config.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@reown/appkit-adapter-wagmi": "^1.7.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "critters": "0.0.23",
    "cssnano": "7.0.6",
    "ethers": "^6.8.1",
    "lru-cache": "^11.1.0",
    "next": "^15.3.0",
    "next-auth": "^4.24.5",
    "pg": "^8.11.3",
    "pino-pretty": "^13.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.55.0",
    "redis": "^4.6.10",
    "wagmi": "^2.14.16",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@types/aws-sdk": "^2.7.4",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "babel-plugin-styled-components": "2.1.4",
    "critter": "1.1.0",
    "eslint": "^8",
    "eslint-config-next": "^15.3.0",
    "hardhat": "^2.19.0",
    "postcss": "^8",
    "styled-components": "6.1.17",
    "tailwindcss": "^3.3.0",
    "ts-node": "10.9.2",
    "typescript": "^5",
    "webpack": "^5.89.0"
  }
}
